@model IList<SurveyResult>
@{
    ViewData["Title"] = "SurveyResults";
}

<h1>Favorite Parks</h1>

<h2 id="temp">@TempData["msg"]</h2>

<div>
    @foreach (SurveyResult result in Model)
    {
        if (result.Count > 1)
        {
            <div>
            <img src="~/images/@(result.ParkCode).jpg" />
            @result.Count
            @result.ParkName
        </div>
        }
    }


</div>
